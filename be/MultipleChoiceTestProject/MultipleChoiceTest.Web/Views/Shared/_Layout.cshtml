                    <div class="menu_background">
                        <div id="search" class="headerSearch">
                            <div class="input-group search_header">
                                <input type="text" class="form-control" maxlength="60" placeholder="Tìm kiếm...">
                                <span class="input-group-btn">
                                    <button type="button" class="btn btn-info">
                                        <img src="/assets/images/icon_search.png">
                                    </button>
                                </span>
                            </div>
                        </div>
                        <div class="navbar navbar-default navbar-static-top">
                            <div class="collapse navbar-collapse" id="menu-site-default">
                                <ul class="nav navbar-nav">
                                    <li>
                                        <a href="/home/index" title="Trang nhất"><span>TRANG CHỦ</span></a>
                                    </li>
                                    <li class="dropdown" rol="presentation">
                                        <a class="dropdown-toggle" role="button" aria-expanded="false"
