                                            </li>
                                        </ul>
                                    </li>
                                    <li rol="presentation">
                                        <a href="" role="button" aria-expanded="false" title="LỊCH SỬ THI">
                                            <span>
                                                LỊCH SỬ
                                                THI
                                            </span>
                                        </a>
                                    </li>
                                    <li rol="presentation">
                                        <a href="/Contact/Contact" role="button" aria-expanded="false" title="LIÊN HỆ"><span>LIÊN HỆ</span></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </nav>
    <div class="main">
        <div class="container">
            @RenderBody()
        </div>
    </div>

    <footer id="footer">
        <div class="footer-copyright">
            <div class="container">
                <div class="row">
                    <div class="col-xs-24 col-sm-24 col-md-12">
                        <div class="panel-body">
                            <div class="powered fr">
                                © Bản quyền thuộc về <a href="https://fb.com/buiphukhuyen" style="color:#ffff00"
                                                        title="Bùi Phú Khuyên" target="_blank" rel="noopener noreferrer nofollow">
                                    Tạ Đức
                                    Hoàn
                                </a>.
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-24 col-sm-24 col-md-12">
                        <div class="panel-body">
                            <div class="theme-change fr">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
